{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node --loader ts-node/esm app/Server.ts",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d db/DataSource.ts",
    "migration:create": "run() { npx typeorm-ts-node-esm migration:create .db/migrations/$1; }; run",
    "migration:generate": "run() { npx typeorm-ts-node-esm migration:generate db/migrations/$1 -d db/DataSource.ts; }; run",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d db/DataSource.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "NODE_OPTIONS='--loader ts-node/esm NODE_ENV=test' mocha --exit 'test/**/*.test.ts'"
  },
  "dependencies": {
    "chess.js": "^1.0.0-beta.6",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "react-chessboard": "^4.0.1",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/chai": "^4.3.6",
    "@types/chess.js": "^0.13.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "chai": "^4.3.8",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-hooks": "^1.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
